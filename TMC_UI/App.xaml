<Application x:Class="TMC_UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TMC_UI"
             >
    <Application.Resources>
        <ResourceDictionary >

            <ResourceDictionary.MergedDictionaries >
                <!--<ResourceDictionary Source="Styles/Style.xaml"/>-->
                <ResourceDictionary Source="Styles/LightTheme.xaml"/>
                <ResourceDictionary Source="Styles/ControlStyles.xaml"/>
                <ResourceDictionary Source="Styles/Font.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <BitmapImage x:Key="OkonLogo" UriSource="Images/OKON_LOGO_grey.png" />
        </ResourceDictionary >
    </Application.Resources>
</Application>
